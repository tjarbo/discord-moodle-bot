{
  "name": "@fmdb/backend",
  "version": "1.0.0",
  "description": "API&Bot backend of the FMDB-Project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rd dist /s /q || rm -r dist/",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run prebuild",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "npm run build",
    "start": "concurrently -c \"yellow.bold,orange.bold\" -n \"WATCH,RUN\" \"npm run build:watch\" \"nodemon --ignore dist/docs/ .\"",
    "test": "jest"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.2.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.12",
    "node-fetch": "^2.6.0",
    "swagger-ui-express": "^4.1.4",
    "tslint": "^6.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-rate-limit": "^5.0.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/helmet": "0.0.46",
    "@types/jest": "^25.2.3",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.7.15",
    "@types/node": "^13.13.8",
    "@types/node-fetch": "^2.5.7",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/ws": "^7.2.4",
    "@types/yamljs": "^0.2.31",
    "babel-jest": "^26.0.1",
    "concurrently": "^5.2.0",
    "jest": "^26.0.1",
    "mockingoose": "^2.13.2",
    "nodemon": "^2.0.3",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3"
  }
}
